{
  "name": "dlimeb.github.io",
  "version": "1.0.0",
  "description": "Personal website at dieter.ca",
  "main": "index.js",
  "scripts": {
    "build:styles": "node-sass assets/styles/src/styles.scss --include-path ./node_modules/normalize.scss/ --stdout --output-style compressed | postcss --use autoprefixer -o assets/styles/styles.css",
    "watch:styles": "watch 'npm run -s build:styles & DEBUG=1 && npm run -s bust-cache' assets/styles/src",
    "watch": "npm run -s watch:styles",
    "server": "browser-sync start --config bs-config.js",
    "start": "npm run server -s & npm run watch -s",
    "bust-cache": "asset-cache-bust *.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlimeb/dlimeb.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "postcss-cli": "6.0.0",
    "autoprefixer": "9.0.1",
    "browser-sync": "2.24.6",
    "node-sass": "4.9.2",
    "watch": "1.0.2",
    "normalize.scss": "0.1.0",
    "asset-cache-bust": "^2.2.0"
  },
  "bugs": {
    "url": "https://github.com/dlimeb/dlimeb.github.io/issues"
  },
  "homepage": "https://github.com/dlimeb/dlimeb.github.io#readme",
  "browserslist": [
    "last 2 versions",
    "safari >= 8",
    "ie 11",
    "not ie 10",
    "not ie_mob 10"
  ]
}
